{
  "name": "Asgard-backend",
  "version": "1.0.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:local": "cross-env HOST=185.178.46.248 nodemon",
    "deploy": "tsc && pm2 restart ecosystem.config.js",
    "deploy:local": "tsc && pm2 restart ecosystem.config.js --env local",
    "deploy:docker": "tsc && pm2-runtime ecosystem.config.js --env local",
    "docker:build": "npm i && tsc && sudo docker build -t exclusivelogin/asgard-back .",
    "docker:publish": "npm run docker:build && sudo docker login && sudo docker push exclusivelogin/asgard-back",
    "docker:run": "sudo docker pull exclusivelogin/asgard-back && sudo docker run --rm -d -p 8091:3000 exclusivelogin/asgard_back"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "multer": "^1.4.2",
    "mysql": "^2.16.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.20",
    "@types/uuid": "^8.3.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.7",
    "pm2": "^4.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
